{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Jaeger": {
    "BaseUrl": "http://localhost:16686/api/traces",
    "TimeoutSeconds": 15
  },
  "Features": {
    "EnableServiceMap": true,
    "EnableTraceComparison": true,
    "EnableTraceExport": true,
    "EnableDashboardAnalytics": true,
    "EnableNotifications": true
  },
  "Redis": {
    "ConnectionString": "localhost:6379",
    "DefaultExpirationSeconds": 3600,
    "NotificationCacheKey": "amin-tahlil:notifications"
  }
}
